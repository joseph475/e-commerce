# Frontend-specific environment variables
# These will override any variables with the same name from the root .env

# Frontend-specific API URL (if different from backend)
REACT_APP_API_URL=http://localhost:8003

# Frontend-specific feature flags
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_ANALYTICS=false

# Frontend-specific theme
REACT_APP_THEME=light

# Frontend-specific version
REACT_APP_VERSION=1.0.0-frontend

# Currency settings
REACT_APP_CURRENCY_SYMBOL=â‚±
REACT_APP_CURRENCY_CODE=PHP
